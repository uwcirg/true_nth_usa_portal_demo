{
  "name": "substudy_tailored_content_application",
  "version": "0.0.1",
  "description": "Substudy Tailored Content Application",
  "author": "CIRG",
  "license": "BSD-Source-Code",
  "scripts": {
    "develop": "webpack --config webpack.config.js --mode development",
    "watch": "webpack --watch --config webpack.config.js --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uwcirg/truenth-portal"
  },
  "nyc": {
    "include": [
      "src/**/*.(js|vue)"
    ],
    "instrument": false,
    "sourceMap": false
  },
  "jest": {
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/runtime": "^7.10.1",
    "@vue/cli-plugin-unit-jest": "^4.5.7",
    "bootstrap": "^5.0.0-alpha1",
    "custom-event-polyfill": "^1.0.7",
    "es6-promise": "^4.2.8",
    "jquery": "^3.5.1",
    "less": "^3.11.1",
    "popper.js": "^1.16.1",
    "vue-router": "^3.4.5",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/runtime-corejs3": "^7.10.2",
    "@vue/test-utils": "^1.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.5.2",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.6.5",
    "css-loader": "^3.6.0",
    "expect": "^26.1.0",
    "expose-loader": "^1.0.0",
    "file-loader": "^6.0.0",
    "filemanager-webpack-plugin": "^7.0.0",
    "html-webpack-plugin": "^4.0.4",
    "jest": "^26.5.2",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "vue": "^2.5.21",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.5.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.21",
    "vue-test-utils": "^1.0.0-beta.11",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^2.3.0"
  }
}
